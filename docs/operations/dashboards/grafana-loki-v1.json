{
  "$schema": "https://grafana.com/schema/dashboard/v1",
  "schema_version": 1,
  "min_ledger_version": "0.5.0",
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "sum by (service, method) (rate({job=\"inferadb-ledger\"} | json | service=~\"$service\" | method=~\"$method\" [$__interval]))",
          "legendFormat": "{{service}}/{{method}}",
          "refId": "A"
        }
      ],
      "title": "Request Rate by Service/Method",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "normal" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "sum by (error_code) (rate({job=\"inferadb-ledger\"} | json | outcome=\"error\" | service=~\"$service\" [$__interval]))",
          "legendFormat": "{{error_code}}",
          "refId": "A"
        }
      ],
      "title": "Error Rate by Error Code",
      "type": "timeseries"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 4,
      "title": "Latency",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 0,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "ms"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*p99.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*p95.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*p50.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 5,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "quantile_over_time(0.50, {job=\"inferadb-ledger\"} | json | service=~\"$service\" | method=~\"$method\" | unwrap duration_ms [$__interval]) by (method)",
          "legendFormat": "{{method}} p50",
          "refId": "A"
        },
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "quantile_over_time(0.95, {job=\"inferadb-ledger\"} | json | service=~\"$service\" | method=~\"$method\" | unwrap duration_ms [$__interval]) by (method)",
          "legendFormat": "{{method}} p95",
          "refId": "B"
        },
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "quantile_over_time(0.99, {job=\"inferadb-ledger\"} | json | service=~\"$service\" | method=~\"$method\" | unwrap duration_ms [$__interval]) by (method)",
          "legendFormat": "{{method}} p99",
          "refId": "C"
        }
      ],
      "title": "Latency Percentiles by Method",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "scaleDistribution": { "type": "linear" }
          },
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 6,
      "options": {
        "calculate": true,
        "calculation": { "xBuckets": { "mode": "size" }, "yBuckets": { "mode": "size", "scale": { "log": 2, "type": "log" } } },
        "cellGap": 1,
        "color": { "exponent": 0.5, "fill": "dark-orange", "mode": "scheme", "reverse": false, "scale": "exponential", "scheme": "Spectral", "steps": 64 },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "legend": { "show": true },
        "rowsFrame": { "layout": "auto" },
        "tooltip": { "show": true, "yHistogram": false },
        "yAxis": { "axisPlacement": "left", "reverse": false, "unit": "ms" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "{job=\"inferadb-ledger\"} | json | service=~\"$service\" | method=~\"$method\" | unwrap duration_ms",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Latency Heatmap",
      "type": "heatmap"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 7,
      "title": "Top Clients & Organizations",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "count" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-BlYlRd" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 8,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "count" }]
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "topk(10, sum by (client_id) (count_over_time({job=\"inferadb-ledger\"} | json | service=~\"$service\" [$__range])))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Top 10 Clients by Request Count",
      "transformations": [
        { "id": "reduce", "options": { "includeTimeField": false, "mode": "reduceFields", "reducers": ["lastNotNull"] } },
        { "id": "sortBy", "options": { "fields": {}, "sort": [{ "desc": true, "field": "Last *" }] } },
        { "id": "limit", "options": { "limitField": 10 } }
      ],
      "type": "table"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "count" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-RdYlGn", "reverse": true } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 9,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "count" }]
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "topk(10, sum by (organization_slug) (count_over_time({job=\"inferadb-ledger\"} | json | outcome=\"error\" [$__range])))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Top 10 Organizations by Error Count",
      "transformations": [
        { "id": "reduce", "options": { "includeTimeField": false, "mode": "reduceFields", "reducers": ["lastNotNull"] } },
        { "id": "sortBy", "options": { "fields": {}, "sort": [{ "desc": true, "field": "Last *" }] } },
        { "id": "limit", "options": { "limitField": 10 } }
      ],
      "type": "table"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 10,
      "title": "Sampling & Efficiency",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "hideFrom": { "legend": false, "tooltip": false, "viz": false } },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Sampled" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Dropped" },
            "properties": [{ "id": "color", "value": { "fixedColor": "gray", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 28 },
      "id": 11,
      "options": {
        "displayLabels": ["name", "percent"],
        "legend": { "displayMode": "list", "placement": "right", "showLegend": true, "values": ["value"] },
        "pieType": "pie",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "count_over_time({job=\"inferadb-ledger\"} | json [$__range])",
          "legendFormat": "Sampled",
          "refId": "A"
        }
      ],
      "title": "Sampling Effectiveness",
      "description": "Shows the number of events emitted. Dropped events are not visible in logs.",
      "type": "piechart"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 28 },
      "id": 12,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "avg_over_time({job=\"inferadb-ledger\"} | json | batch_coalesced=\"true\" | unwrap batch_size [$__interval])",
          "legendFormat": "Avg Batch Size",
          "refId": "A"
        }
      ],
      "title": "Batch Coalescing Efficiency",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "loki", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 28 },
      "id": 13,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "sum(count_over_time({job=\"inferadb-ledger\"} | json | idempotency_hit=\"true\" [$__interval])) / sum(count_over_time({job=\"inferadb-ledger\"} | json | method=\"write\" [$__interval]))",
          "legendFormat": "Idempotency Hit Rate",
          "refId": "A"
        }
      ],
      "title": "Idempotency Cache Hit Rate",
      "type": "timeseries"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "id": 14,
      "title": "Detailed Breakdown",
      "type": "row",
      "collapsed": true,
      "panels": [
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisBorderShow": false,
                "axisCenteredZero": false,
                "axisColorMode": "text",
                "axisLabel": "",
                "axisPlacement": "auto",
                "barAlignment": 0,
                "drawStyle": "line",
                "fillOpacity": 10,
                "gradientMode": "none",
                "hideFrom": { "legend": false, "tooltip": false, "viz": false },
                "lineInterpolation": "linear",
                "lineWidth": 1,
                "pointSize": 5,
                "scaleDistribution": { "type": "linear" },
                "showPoints": "never",
                "spanNulls": false,
                "stacking": { "group": "A", "mode": "none" },
                "thresholdsStyle": { "mode": "off" }
              },
              "mappings": [],
              "thresholds": {
                "mode": "absolute",
                "steps": [{ "color": "green", "value": null }]
              },
              "unit": "ms"
            },
            "overrides": []
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 37 },
          "id": 15,
          "options": {
            "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
            "tooltip": { "mode": "multi", "sort": "desc" }
          },
          "targets": [
            {
              "datasource": { "type": "loki", "uid": "${datasource}" },
              "expr": "avg_over_time({job=\"inferadb-ledger\"} | json | method=\"write\" | unwrap raft_latency_ms [$__interval])",
              "legendFormat": "Raft Latency",
              "refId": "A"
            },
            {
              "datasource": { "type": "loki", "uid": "${datasource}" },
              "expr": "avg_over_time({job=\"inferadb-ledger\"} | json | unwrap storage_latency_ms [$__interval])",
              "legendFormat": "Storage Latency",
              "refId": "B"
            }
          ],
          "title": "Latency Breakdown (Raft vs Storage)",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "loki", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "axisBorderShow": false,
                "axisCenteredZero": false,
                "axisColorMode": "text",
                "axisLabel": "",
                "axisPlacement": "auto",
                "barAlignment": 0,
                "drawStyle": "bars",
                "fillOpacity": 100,
                "gradientMode": "none",
                "hideFrom": { "legend": false, "tooltip": false, "viz": false },
                "lineInterpolation": "linear",
                "lineWidth": 1,
                "pointSize": 5,
                "scaleDistribution": { "type": "linear" },
                "showPoints": "never",
                "spanNulls": false,
                "stacking": { "group": "A", "mode": "normal" },
                "thresholdsStyle": { "mode": "off" }
              },
              "mappings": [],
              "thresholds": {
                "mode": "absolute",
                "steps": [{ "color": "green", "value": null }]
              },
              "unit": "short"
            },
            "overrides": [
              {
                "matcher": { "id": "byName", "options": "success" },
                "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
              },
              {
                "matcher": { "id": "byName", "options": "error" },
                "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
              },
              {
                "matcher": { "id": "byName", "options": "cached" },
                "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
              },
              {
                "matcher": { "id": "byName", "options": "rate_limited" },
                "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
              },
              {
                "matcher": { "id": "byName", "options": "precondition_failed" },
                "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
              }
            ]
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 37 },
          "id": 16,
          "options": {
            "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true },
            "tooltip": { "mode": "multi", "sort": "desc" }
          },
          "targets": [
            {
              "datasource": { "type": "loki", "uid": "${datasource}" },
              "expr": "sum by (outcome) (count_over_time({job=\"inferadb-ledger\"} | json | service=~\"$service\" [$__interval]))",
              "legendFormat": "{{outcome}}",
              "refId": "A"
            }
          ],
          "title": "Request Outcomes",
          "type": "timeseries"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["inferadb", "wide-events", "loki"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "", "value": "" },
        "hide": 0,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "loki",
        "queryValue": "",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
